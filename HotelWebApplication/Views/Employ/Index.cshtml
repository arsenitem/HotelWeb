@model List<string[]>
@{
    ViewBag.Title = "Сотрудники";
}
<div class="row text-center">
    <div class="col-md-8">
        <h2>Текущие гости</h2>
        <table class="table">
            <tr id="head">
                <th id="headline">ID</th>
                <th id="headline">ФИО</th>
                <th id="headline">Телефон</th>
                <th id="headline">Должность</th>
                <th id="headline">Оклад</th>

            </tr>
            <tbody id="tbody">
                @foreach (var res in Model)
                 {
            <tr>
                <td>@res[0]</td>
                <td>@res[1]</td>
                <td>@res[2]</td>
                <td>@res[3]</td>
                <td>@res[4]</td>
               
            </tr>
                 }
            </tbody>
        </table>
    </div>
    <div class="col-md-4" id="emp_left">
    @Ajax.ActionLink("Добавить сотрудника", "AddEmployeeView", new AjaxOptions { UpdateTargetId = "emp_left" })
   <a id="del_link" onclick="DeleteEmp()"> Удалить сотрудника</a>
    </div>
</div>
